package Class_4;

public class M_overLoad_2 {

	void add()
	{
	System.out.println("My name is Fabiha !!!");
	}
	int add(int a , int b)
	{
		return a+b;
	}
	
	int add(int a , int b, int c)
	{
		return a+b+c;
	}
	
	float add(float a , float b, float c, float d)
	{
		return a+b+c+d;
	}
	
}
